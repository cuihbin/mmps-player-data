<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="playerKeyDao" class="org.appfuse.dao.hibernate.GenericDaoHibernate">
    <constructor-arg value="com.zzvc.mmps.model.PlayerKey" />
    <property name="sessionFactory" ref="sessionFactory" />
  </bean>

  <bean id="playerKeyValueDao" class="org.appfuse.dao.hibernate.GenericDaoHibernate">
    <constructor-arg value="com.zzvc.mmps.model.PlayerKeyValue" />
    <property name="sessionFactory" ref="sessionFactory" />
  </bean>

</beans>